local soundeffect1 = Instance.new("Sound")
                soundeffect1.SoundId = "rbxassetid://6995347277"
                soundeffect1.Parent = game.Players.LocalPlayer.Character.Torso
                soundeffect1.Volume = 4.5

local tool = Instance.new("Tool")
	tool.Name = "Instant Dodge"
	tool.Parent = game.Players.LocalPlayer.Backpack
	tool.RequiresHandle = false
	tool.CanBeDropped = false

	tool.Equipped:Connect(function()
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local character = player.Character
		local currentPivot = character:GetPivot()
		soundeffect1:Play()
		character:PivotTo(currentPivot * CFrame.new(25, 0, 0))
	end)


local UIS = game:GetService("UserInputService")

local soundeffect2 = Instance.new("Sound")
                soundeffect2.SoundId = "rbxassetid://3084314259"
                soundeffect2.Parent = game.Players.LocalPlayer.Character.Torso
                soundeffect2.Volume = 4.5

UIS.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.T then
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local character = player.Character
		local currentPivot = character:GetPivot()
		soundeffect:Play()
		character:PivotTo(currentPivot * CFrame.new(0, 0, -25))
	end
end)


local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local debounce = false

uis.InputBegan:Connect(function(key, processed)
	if debounce or processed then
		return
	end

	if key.KeyCode == Enum.KeyCode.V then
		debounce = true
		humanoid.WalkSpeed += 14
		task.wait(0.5)
		humanoid.WalkSpeed -= 14
	end

	task.wait(1.5)
	debounce = false
end)
